        <Button Content="Применить" HorizontalAlignment="Left" Margin="122,370,0,0" VerticalAlignment="Top" Width="180" Height="40" FontSize="16" Click="Button_Click"/>
        <Button Content="Сохранить" HorizontalAlignment="Left" Margin="307,370,0,0" VerticalAlignment="Top" Width="180" Height="40" FontSize="16" Click="Button_Click_1"/>
        <Button Content="Отмена" HorizontalAlignment="Left" Margin="492,370,0,0" VerticalAlignment="Top" Width="180" Height="40" FontSize="16"/>
        <Label Content="Тип счета" HorizontalAlignment="Left" Margin="417,323,0,0" VerticalAlignment="Top" Width="320" FontSize="16" Height="30" Padding="0"/>
        <ComboBox x:Name="ComboBox2" HorizontalAlignment="Left" Margin="96,323,0,0" VerticalAlignment="Top" Width="285" Height="25">
            <TextBlock>Deposit</TextBlock>
            <TextBlock>noDeposit</TextBlock> 
        </ComboBox>
